  "description": "A simple react grid component that is highly extendable",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ra0o0f/react-extendable-grid.git"
  },
  "keywords": [
    "react",
    "component",
    "grid",
    "typescript"
  ],
