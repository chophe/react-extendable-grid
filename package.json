    "gulp-less": "^3.1.0",
    "gulp-rename": "^1.2.2",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "rimraf": "^2.5.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
